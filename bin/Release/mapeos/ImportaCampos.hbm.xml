<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="Exitosw.Payroll.Hibernate" namespace="Exitosw.Payroll.Hibernate.entidad" xmlns="urn:nhibernate-mapping-2.2">
  <class name="ImportaCampos" table="ImportaCampos" >
    <id name="id" type="Decimal">
      <column name="id" not-null="true" precision="19" scale="0" sql-type="NUMERIC(19)" />
      <generator class="identity" />
    </id>
    <property name="clave" type="String">
      <column name="clave" not-null="true" length="30" sql-type="VARCHAR(30)" />
    </property>
    <property name="fila" type="Int32">
      <column name="fila" not-null="false" precision="10" scale="0" sql-type="INT" />
    </property>
    <property name="nombreConfiguracion" type="String">
      <column name="nombreConfiguracion" not-null="false" length="255" sql-type="VARCHAR(255)" />
    </property>
    <property name="nombreVentana" type="String">
      <column name="nombreVentana" not-null="false" length="255" sql-type="VARCHAR(255)" />
    </property>
  </class>
</hibernate-mapping>