<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="Exitosw.Payroll.Hibernate" namespace="Exitosw.Payroll.Hibernate.entidad" xmlns="urn:nhibernate-mapping-2.2">
  <class name="DetalleImportaCampos" table="DetalleImportaCampos" >
    <id name="id" type="Decimal">
      <column name="id" not-null="true" precision="19" scale="0" sql-type="NUMERIC(19)" />
      <generator class="identity" />
    </id>
    <property name="campo" type="String">
      <column name="campo" not-null="false" length="255" sql-type="VARCHAR(255)" />
    </property>
    <property name="noColumna" type="Int32">
      <column name="noColumna" not-null="true" precision="10" scale="0" sql-type="INT" />
    </property>
    <property name="orden" type="Int32">
      <column name="orden" not-null="false" precision="10" scale="0" sql-type="INT" />
    </property>
    <property name="importaCamposID" type="Decimal">
      <column name="importaCampos_ID" not-null="false" precision="19" scale="0" sql-type="NUMERIC(19)" />
    </property>
  </class>
</hibernate-mapping>