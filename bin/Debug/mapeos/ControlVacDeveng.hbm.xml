<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="Exitosw.Payroll.Hibernate" namespace="Exitosw.Payroll.Hibernate.entidad" xmlns="urn:nhibernate-mapping-2.2">
  <class name="ControlVacDeveng" table="ControlVacDeveng" >
    <id name="id" type="Decimal">
      <column name="id" not-null="true" precision="19" scale="0" sql-type="NUMERIC(19)" />
      <generator class="identity" />
    </id>
    <property name="fecha" type="DateTime">
      <column name="fecha" not-null="false" sql-type="DATETIME" />
    </property>
    <many-to-one name="razonesSociales" class="RazonesSociales">
      <column name="razonesSociales_ID" not-null="true" precision="19" scale="0" sql-type="NUMERIC(19)" />
    </many-to-one>
  </class>
</hibernate-mapping>